<% if user_signed_in? %>
  <li><%= link_to t('botonera.edit'), edit_user_registration_path %></li>
  <li><%= link_to t('botonera.signout'), destroy_user_session_path, :method=>'delete' %></li>
<% else %>
  <li><%= link_to t('botonera.signin'), new_user_session_path %></li>
  <li><%= link_to t('botonera.signup'), new_user_registration_path %></li>
<% end %>

